{
  "rewrites": [
    {
      "source": "/api/threads",
      "destination": "/api/threads"
    },
    {
      "source": "/api/replies",
      "destination": "/api/replies"
    }
  ],
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "/api/*.js",
      "use": "@vercel/node"
    }
  ]
}